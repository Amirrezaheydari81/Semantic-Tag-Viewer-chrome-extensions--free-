{
    "manifest_version": 3,
    "name": "Semantic Structure Highlighter",
    "version": "1.4",
    "description": "نمایش تگ‌های سمانتیک HTML در صفحه وب با قابلیت روشن/خاموش",
    "permissions": ["activeTab", "scripting", "storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {

      }
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
